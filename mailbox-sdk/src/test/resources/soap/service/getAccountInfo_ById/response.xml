<?xml version='1.0' encoding='UTF-8'?>
<soap:Envelope xmlns:soap="http://www.w3.org/2003/05/soap-envelope">
    <soap:Header>
        <context xmlns="urn:zimbra">
            <change token="2463"/>
        </context>
    </soap:Header>
    <soap:Body>
        <GetAccountInfoResponse xmlns="urn:zimbraAccount">
            <name>%ACCOUNT_EMAIL%</name>
            <attr name="zimbraId">%ACCOUNT_ID%</attr>
            <attr name="zimbraMailHost">%ACCOUNT_DOMAIN%</attr>
            <attr name="displayName">%ACCOUNT_NAME%</attr>
            <soapURL>http://server.%ACCOUNT_DOMAIN%/service/soap/</soapURL>
            <publicURL>http://server.%ACCOUNT_DOMAIN%</publicURL>
        </GetAccountInfoResponse>
    </soap:Body>
</soap:Envelope>
