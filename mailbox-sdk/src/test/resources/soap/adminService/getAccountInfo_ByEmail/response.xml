        <context xmlns="urn:zimbra">
            <change token="2463"/>
        </context>
    </soap:Header>
    <soap:Body>
        <GetAccountInfoResponse xmlns="urn:zimbraAdmin">
            <name>%ACCOUNT_EMAIL%</name>
