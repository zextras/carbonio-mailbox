<zimbraDirSync>    
    <masterURL>ldap://exchange55.example.com</masterURL>
    <masterFilter>objectclass=person</masterFilter>
    <masterSearchBase>o=EXAMPLE</masterSearchBase>
    <masterBindUser>cn=administrator,cn=users,dc=example,dc=com</masterBindUser>
    <masterBindPassword>test123</masterBindPassword>
    <masterBindTimeout>20</masterBindTimeout>
    <logLevel>2</logLevel>
    <requiredMasterAttribute attr='sn'></requiredMasterAttribute>
    <localIgnore type="smtp">aa@example.com</localIgnore>
    <masterIgnore type="smtp">test0@example.com</masterIgnore>
    <localPassword type="static">test123</localPassword>
    <localDeleteAdmin>false</localDeleteAdmin>
    <deletionScriptFile>del_sh</deletionScriptFile>
    <domains>example.com</domains>
	<localCos>testcos</localCos>
    <attributeMap>
        <attr local="displayName" master="name"/>
    </attributeMap>
</zimbraDirSync>
